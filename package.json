    "cookie-parser": "~1.4.3",
    "express-session": "~1.15.6"
  },
  "devDependencies": {
    "jasmine-core": "^3.1.0",
    "karma": "^2.0.0",
    "karma-chrome-launcher": "^2.2.0",
    "karma-jasmine": "^1.1.1",
    "nodemon": "^1.17.1",
    "protractor": "^5.3.0"
  },
  "keywords": [
    "AngularJS",
    "test-project",
    "demo-app"
