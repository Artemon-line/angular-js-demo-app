{
  "name": "angular-js-demo-app",
  "version": "1.0.0",
  "engines": {
    "npm": "5.7.x"
  },
  "description": "Notes app - example of usage AngularJs",
  "main": "/server/server.js",
  "directories": {
    "src": "src",
    "test": "test"
  },
  "scripts": {
    "start": "node server/server.js",
    "start_dev": "nodemon server/server.js",
    "test": "node ./node_modules/karma/bin/karma start test/unit/karma.conf.js --single-run",
    "e2e": "node ./node_modules/protractor/bin/protractor ./test/e2e/protractor.conf.js",
    "webdriver-update": "webdriver-manager update --standalone false --gecko false"
  },
  "dependencies": {
    "angular": "1.7.2",
    "angular-route": "1.7.2",
    "body-parser": "^1.18.2",
    "bootstrap": "4.3.1",
    "cookie-parser": "~1.4.3",
    "express": "~4.16.2",
    "express-session": "~1.15.6",
    "jquery": "^3.3.1",
    "popper.js": "1.14.3",
    "opn": "^5.3.0",
    "sha1": "^1.1.1"
  },
  "devDependencies": {
    "jasmine-core": "^3.1.0",
    "jasmine-spec-reporter": "^4.2.1",
    "karma": "^4.1.0",
    "karma-chrome-launcher": "^2.2.0",
    "karma-jasmine": "^1.1.1",
    "nodemon": "^1.17.5",
    "protractor": "^6.0.0"
  },
  "keywords": [
    "AngularJS",
    "test-project",
    "demo-app"
  ],
  "author": "Artemon-line",
  "license": "ISC"
}
