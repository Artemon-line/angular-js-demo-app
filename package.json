{
  "name": "angular-js-demo-app",
  "version": "1.0.0",
  "description": "Notes app - example of usage AngularJs",
  "main": "/server/server.js",
  "directories": {
    "src": "src",
    "test": "test"
  },
  "scripts": {
    "build": "nodemon server/server.js",
